# Socrata API token (data.wa.gov) - free, provides higher rate limits
# Get one at: https://data.wa.gov/profile/edit/developer_settings
SOCRATA_APP_TOKEN=your_token_here

# Google AI API key for Gemini chatbot
# Get one at: https://aistudio.google.com/apikey
GOOGLE_API_KEY=your_key_here

# Optional: Anthropic API key (not used by default)
ANTHROPIC_API_KEY=your_key_here

# Optional: OpenAI API key as fallback
OPENAI_API_KEY=your_key_here
